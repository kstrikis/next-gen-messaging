{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "dotenvx run --env-file=.env.development -- node server.js",
    "build": "dotenvx run --env-file=.env -- next build",
    "start": "dotenvx run --env-file=.env -- echo \"Starting client...\" && node server.js",
    "start:test": "dotenvx run --env-file=.env.test -- echo \"üîç Starting client in test mode...\" && node server.js",
    "debug:env": "echo \"Client Before dotenvx:\" && printenv | grep -E 'NODE_ENV|DATABASE_URL' && echo \"\nClient After dotenvx:\" && dotenvx run --env-file=.env.test -- printenv | grep -E 'NODE_ENV|DATABASE_URL'",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:related": "jest --findRelatedTests",
    "test:ci": "jest --ci --coverage",
    "test:update": "jest -u",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\""
  },
  "dependencies": {
